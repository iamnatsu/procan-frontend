    "@types/react-dom": "^16.0.8",
    "@types/react-redux": "^6.0.9",
    "@types/react-router": "^4.0.31",
    "@types/react-select": "^2.0.11",
    "@types/redux": "^3.6.0",
    "@types/redux-form": "^7.4.8",
    "@types/typescript": "^2.0.0",
    "bestzip": "^2.1.2",
    "cpx": "^1.5.0",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
