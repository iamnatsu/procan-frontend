{
  "name": "procan-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all clean init:dir build:module copy:html copy:assets build:cache:bust zip",
    "build:module": "webpack --progress --colors --config ./webpack.config.prod.js",
    "build:cache:bust": "sed -i -e \"s/app.js/app.js?`openssl rand -hex 10`/g\" ./build/tmp/index.html && sed -i -e \"s/style.css/style.css?`openssl rand -hex 10`/g\" ./build/tmp/index.html",
    "copy:html": "cpx ./app/index.html ./build/tmp && cpx ./app/favicon.ico ./build/tmp",
    "copy:assets": "cp -r ./app/locales ./build/tmp/dist/locales",
    "init:dir": "mkdirp build build/tmp build/tmp/dist build/results",
    "clean:": "rimraf build/tmp/* app/dist/scripts/* app/dist/style/* coverage/* build/results/pjm-frontend.zip",
    "zip": "cd build/tmp && bestzip ../results/procan-frontend.zip *",
    "watch": "cp -r ./app/locales/ ./app/dist/ & webpack --progress --colors --watch --config ./webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@date-io/date-fns": "0.0.2",
    "@date-io/moment": "0.0.2",
    "@material-ui/core": "^3.2.0",
    "@material-ui/icons": "^3.0.1",
    "@types/i18next": "^12.1.0",
    "@types/react-dnd-touch-backend": "^0.4.2",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-router-redux": "^5.0.16",
    "axios": "^0.18.0",
    "date-fns": "^2.0.0-alpha.21",
    "i18next": "^12.1.0",
    "immutable": "^3.8.2",
    "material-ui": "^0.20.2",
    "material-ui-pickers": "^2.0.5",
    "moment": "^2.22.2",
    "react": "~16.8.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dnd": "^6.0.0",
    "react-dnd-html5-backend": "^6.0.0",
    "react-dnd-touch-backend": "^0.6.0",
    "react-dom": "^16.5.2",
    "react-i18next": "^8.3.8",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-select": "^2.3.0",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-form-material-ui": "^5.0.0-beta.3",
